{
	"name": "destiny-bounty-group",
	"description": "A website to show available bounties grouped by objective",
	"version": "0.1.0",
	"license": "MIT",
	"private": false,
	"keywords": [
		"typescript",
		"svelte",
		"destiny",
		"bungie"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/lliksis/destiny-bounty-group"
	},
	"bugs": "https://github.com/lliksis/destiny-bounty-group/issues",
	"author": "lliksis",
	"contributors": [
		{
			"name": "lliksis",
			"url": "https://lliksis.dev"
		},
		{
			"name": "Alex25011"
		}
	],
	"scripts": {
		"dev": "svelte-kit dev --https --port 5000",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview --https --port 5000",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@sveltestack/svelte-query": "1.6.0",
		"bungie-api-ts": "4.16.0",
		"cookie": "0.5.0",
		"localforage": "1.10.0",
		"sirv-cli": "1.0.14",
		"uuid": "8.3.2"
	},
	"devDependencies": {
		"@babel/core": "7.17.10",
		"@playwright/test": "1.21.0",
		"@storybook/addon-actions": "6.5.0",
		"@storybook/addon-essentials": "6.5.0",
		"@storybook/addon-interactions": "6.5.0",
		"@storybook/addon-links": "6.5.0",
		"@storybook/builder-webpack4": "6.5.0",
		"@storybook/manager-webpack4": "6.5.0",
		"@storybook/svelte": "6.5.0",
		"@storybook/testing-library": "0.0.11",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "0.5.1",
		"@types/uuid": "8.3.4",
		"@typescript-eslint/eslint-plugin": "5.10.1",
		"@typescript-eslint/parser": "5.10.1",
		"babel-loader": "8.2.2",
		"eslint": "8.12.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-storybook": "0.5.12",
		"eslint-plugin-svelte3": "4.0.0",
		"prettier": "2.5.1",
		"prettier-plugin-svelte": "2.5.0",
		"svelte": "3.44.3",
		"svelte-check": "2.2.11",
		"svelte-loader": "3.1.2",
		"svelte-overlay": "1.4.1",
		"svelte-preprocess": "4.10.1",
		"tslib": "2.4.0",
		"typescript": "4.6.4"
